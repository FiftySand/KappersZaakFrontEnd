rd v-else style=\"visibility: hidden\"></barbercard>\n\n      <b-col align-h=\"end\" style=\"background-color:rgba(200,200,200, 0.5);\">\n          <b-form-select :options=\"kappers\" v-on:change=\"getSelectedItem\"></b-form-select>\n      </b-col>\n    </b-row>\n    </b-container>\n\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nimport kapperCard from \"@/components/kapperCard.vue\"\n\nexport default {\n  components:{\n        'barbercard': kapperCard,\n  },\n  methods:\n  {\n      getSelectedItem: function(myarg) { // Just a regular js function that takes 1 arg\n        this.list.forEach(kapper =>\n        {\n          if(kapper.kapper.id == myarg)\n          {\n            this.activeKapper.id = myarg,\n              this.activeKapper.Name = kapper.kapper.name,\n              this.activeKapper.Age = kapper.kapper.age,\n\n              this.activatedCard = true\n         }\n        })\n      },\n  },\n  data(){\n    return{\n      activatedCard: false,\n      list: [],\n      kappers: [],\n      activeKapper: {\n        Id: '',\n        Name: '',\n        Age: '',\n        \n      }\n    }\n  },\n  mounted () {\n\n    axios\n    \n      .get('http://localhost:9999/kapper')\n      .then(response => {\n        this.list = response.data._embedded.kapperResourceList,\n          this.list.forEach(kapper => {\n          \n          this.kappers.push(\n          {\n            value: kapper.kapper.id,\n            text: kapper.kapper.name\n          }\n                    \n          )})\n      })\n        \n            console.log(this.kappers);\n}\n  }\n\n</script>\n\n<style lang=\"scss\" scoped>\n.my-coll{\n  border: solid \n}\n</style>"]}]}